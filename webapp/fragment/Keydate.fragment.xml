<core:FragmentDefinition
	xmlns="sap.m"
	xmlns:core="sap.ui.core">
	<SelectDialog multiSelect="false"
		title="Open on Key Date"
		class="sapUiPopupWithPadding"
		items="{/P_DateFunctionSet}"
		search="SearchP_DateFunction"
		confirm="ConfP_DateFunction"
		cancel="handleValueHelpClose" growingThreshold="true" growing="true" >
		<StandardListItem
			title="{Datefunction}"
		    description="{Datefunctionname}" 
			/>
	</SelectDialog>
</core:FragmentDefinition>